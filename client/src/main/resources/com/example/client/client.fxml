<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.client.GameController">
    <VBox>
        <TilePane alignment="CENTER" prefColumns="3">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <!--        <Label fx:id="welcomeText"/>-->
            <VBox>
                <HBox>
                    <Button fx:id="btn1" prefWidth="100" prefHeight="100" onAction="#onButtonClick1"/>
                    <Button fx:id="btn2" prefWidth="100" prefHeight="100" onAction="#onButtonClick2"/>
                    <Button fx:id="btn3" prefWidth="100" prefHeight="100" onAction="#onButtonClick3"/>
                </HBox>
                <HBox>
                    <Button fx:id="btn4" prefWidth="100" prefHeight="100" onAction="#onButtonClick4"/>
                    <Button fx:id="btn5" prefWidth="100" prefHeight="100" onAction="#onButtonClick5"/>
                    <Button fx:id="btn6" prefWidth="100" prefHeight="100" onAction="#onButtonClick6"/>
                </HBox>
                <HBox>
                    <Button fx:id="btn7" prefWidth="100" prefHeight="100" onAction="#onButtonClick7"/>
                    <Button fx:id="btn8" prefWidth="100" prefHeight="100" onAction="#onButtonClick8"/>
                    <Button fx:id="btn9" prefWidth="100" prefHeight="100" onAction="#onButtonClick9"/>
                </HBox>
            </VBox>
        </TilePane>
    </VBox>
</VBox>
